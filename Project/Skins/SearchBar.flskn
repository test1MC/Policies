<?xml version="1.0" encoding="utf-8"?>
<CatapultSkin Version="3" SkinType="Component" Comment="HTML5 Search Bar skin" ComponentType="SearchBar" EnableResponsiveOutput="True">
    <Stylesheet>
        <Styles>
            <Style Name="Search Bar">
                <Properties>
                    <Property Name="BorderTopLeftRadius">5px</Property>
                    <Property Name="BorderTopRightRadius">5px</Property>
                    <Property Name="BorderBottomRightRadius">5px</Property>
                    <Property Name="BorderBottomLeftRadius">5px</Property>
                    <Property Name="Width">500px</Property>
                    <Property Name="Height">32px</Property>
                    <Property Name="MarginLeft">0px</Property>
                    <Property Name="MarginTop">0px</Property>
                </Properties>
            </Style>
            <Style Name="Search Submit Button">
                <Properties>
                    <Property Name="BackgroundImage">url('24px-search-white.png')</Property>
                    <Property Name="BackgroundGradient">vertical #555 #999</Property>
                    <Property Name="BackgroundColor">#666</Property>
                    <Property Name="Width">3em</Property>
                </Properties>
            </Style>
            <Style Name="Search Filter Drop-down">
                <Properties>
                    <Property Name="Color">#696969</Property>
                    <Property Name="BackgroundGradient">vertical #ffffff #ffffff</Property>
                    <Property Name="BackgroundColor">#000000</Property>
                </Properties>
            </Style>
            <Style Name="Search Filter (selected)">
                <Properties>
                    <Property Name="BackgroundColor">#000000</Property>
                    <Property Name="BackgroundGradient">vertical #afddfb #afddfb</Property>
                </Properties>
            </Style>
            <Style Name="Search Filter Drop-down (hover)">
                <Properties>
                    <Property Name="BackgroundGradient">vertical #2199e8 #2199e8</Property>
                    <Property Name="Color">#ffffff</Property>
                </Properties>
            </Style>
            <Style Name="Search Filter">
                <Properties>
                    <Property Name="BackgroundGradient">vertical transparent transparent</Property>
                    <Property Name="BorderLeft">solid 0px #ffffff</Property>
                    <Property Name="BorderRight">solid 0px #ffffff</Property>
                </Properties>
            </Style>
            <Medium Name="Mobile">
                <Style Name="Search Bar">
                    <Properties>
                        <Property Name="Width">90%</Property>
                    </Properties>
                </Style>
            </Medium>
            <Style Name="Search Submit Button (hover)">
                <Properties>
                    <Property Name="BackgroundGradient">vertical #999 #555</Property>
                    <Property Name="BackgroundColor">#999</Property>
                    <Property Name="BackgroundImage">url('24px-search-white.png')</Property>
                </Properties>
            </Style>
            <Style Name="Search Input">
                <Properties>
                    <Property Name="BackgroundColor">transparent</Property>
                    <Property Name="BackgroundGradient">vertical #f5f5f5 #ffffff</Property>
                    <Property Name="PaddingLeft">15px</Property>
                    <Property Name="FontSize">17px</Property>
                </Properties>
            </Style>
        </Styles>
        <Resources>
            <Resource Name="24px-search-white.png"></Resource>
        </Resources>
        <Stylesheet>
        </Stylesheet>
    </Stylesheet>
    <Strings>
        <Stylesheet lang="en-us">
            <Stylesheet>
